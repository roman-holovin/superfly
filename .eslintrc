{
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "jsx": true,
    }
  },
  "settings": {
    "react": {
      "pragma": "h"
    }
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jest": true
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2
  }
}
